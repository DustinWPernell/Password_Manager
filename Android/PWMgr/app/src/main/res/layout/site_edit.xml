<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/SiteHeaderSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/PWHeaderSE"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:text="Site:"
        android:textSize="25dp" />

    <EditText
        android:id="@+id/editSiteSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/SiteHeaderSE"
        android:layout_alignBottom="@+id/SiteHeaderSE"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@+id/SiteHeaderSE"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Site Name"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/PWHeaderSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password:"
        android:textSize="25dp"
        android:layout_below="@+id/SiteHeaderSE"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp" />

    <EditText
        android:id="@+id/editPWSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/PWHeaderSE"
        android:layout_alignBottom="@+id/PWHeaderSE"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@+id/PWHeaderSE"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Password"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/DateViewSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/DateHeaderSE"
        android:layout_alignBottom="@+id/DateHeaderSE"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@+id/DateHeaderSE"
        android:text="Not Set"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/DateHeaderSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date:"
        android:textSize="25dp"
        android:layout_below="@+id/PWHeaderSE"
        android:layout_alignEnd="@+id/PWHeaderSE"
        android:layout_marginTop="30dp" />


    <Button
        android:id="@+id/SaveButtonSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:text="Save" />

    <Button
        android:id="@+id/CancelButtonSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:text="Cancel" />

    <TextView
        android:id="@+id/SecQuestHeaderSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/DateViewSE"
        android:layout_marginTop="14dp"
        android:text="Securit Question:"
        android:textSize="25dp" />

    <Button
        android:id="@+id/AddSecQuestBNuttonSE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Security Question"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>